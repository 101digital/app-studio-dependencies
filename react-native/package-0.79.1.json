{
  "name": "pay101V2DemoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "e2e:dev-test-android": "./run_maestro_test.sh android",
    "e2e:staging-test-android": "./run_maestro_test.sh android staging",
    "e2e:prod-test-android": "./run_maestro_test.sh android prod",
    "e2e:dev-test-ios": "./run_maestro_test.sh ios",
    "e2e:staging-test-ios": "./run_maestro_test.sh ios staging",
    "e2e:prod-test-ios": "./run_maestro_test.sh ios prod",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest --coverage",
    "android-dev": "react-native run-android --mode=developmentDebug --appIdSuffix dev",
    "ios-dev": "react-native run-ios --scheme=pay101V2DemoApp-Development",
    "android-staging": "react-native run-android --mode=stagingDebug --appIdSuffix staging",
    "ios-staging": "react-native run-ios --scheme=pay101V2DemoApp-Staging",
    "android-prod": "react-native run-android --mode=productionDebug",
    "ios-prod": "react-native run-ios --scheme=pay101V2DemoApp-Production",
    "postinstall": "patch-package && npx react-native-asset",
    "format": "npx prettier --write .",
    "build-android": "export ENVFILE=.env.staging && cd android && ./gradlew assembleRelease",
    "install-pod": "bundle install && arch -x86_64 pod install"
  },
